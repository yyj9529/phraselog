    "@types/nprogress": "^0.2.3",
    "@types/pg": "^8.11.11",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.1",
    "cross-env": "^10.0.0",
    "drizzle-kit": "^0.30.5",
    "prettier": "^3.5.3",
    "prettier-plugin-tailwindcss": "^0.6.11",
    "react-router-devtools": "^1.1.0",
    "supabase": "^2.40.7",
    "tailwindcss": "^4.0.17",
    "typescript": "^5.7.2",
    "vite": "^5.4.11",
    "vite-tsconfig-paths": "^5.1.4"
  }
